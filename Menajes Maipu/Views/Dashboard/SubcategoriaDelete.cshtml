@model Menajes_Maipu.Models.ModeloBD.Subcategoria
  
@{
    var user = Session["user"];
    var id = Session["id"];

    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Ladmin.cshtml";
}

<!-- page content -->
<div class="right_col" role="main">

        <div class="">
            <div class="page-title">
                <div class="title_left">
                    <h3>
                        Eliminar Subcategoria
                    </h3>
                </div>


            </div>
            <div class="clearfix"></div>

            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h2>Subcategoria <small>Eliminar</small></h2>
                         
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">

                            @using (Html.BeginForm("", "", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal form-label-left" }))
                            {

                                <div class="item form-group">
                                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="name">
                                        Nombre <span class="required"></span>
                                    </label>
                                    <div class="col-md-6 col-sm-6 col-xs-12">
                                        @foreach (var item in ViewBag.nombre)
                                        {
                                            @Html.TextBoxFor(model => model.Nombre_subcategoria, new { @readonly = "readonly", @Value = @item.Nombre_subcategoria, @class = "form-control" })

                                        }
                                    </div>
                                </div>
                            }              <span class="section"></span>




                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()

                                <div class="form-actions no-color" align="Center">
                                    <input type="submit" value="Eliminar" class="btn btn-danger" />
                                    @Html.ActionLink("Volver al listado", "SubcategoriaList","Dashboard", new { }, new { @class = "btn btn-info" })
                                </div>
                            }




                        </div>
                    </div>
                </div>
            </div>
         </div>

       <!-- footer content -->
       <footer>
                        <div class="">
                            <p class="pull-right">
                                <a>RGL</a>
                            </p>
                        </div>
                        <div class="clearfix"></div>
                    </footer>
       <!-- /footer content -->          
     </div>
<!-- /page content -->