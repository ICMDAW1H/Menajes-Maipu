            <div class="page-title">
                <div class="title_left">
                    <h3>
                       Productos destacados
                        @Html.ActionLink("Volver al listado", "ProductosList", "Dashboard", new { }, new { @class = "btn btn-info" })

                        @*<small>
                                Some examples to get you started
                            </small>*@
                    </h3>
                </div>


            </div>
