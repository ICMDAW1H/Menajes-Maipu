                    <div class="x_panel">
                        <div class="x_title">
                            <b>PRINCIPAL</b>
                            <ul class="nav navbar-right panel_toolbox">
                                <li>
                                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                </li>
                             
                            </ul>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content" align="center">
                            <table class="table table-responsive" align="center">
                                <thead>
                                    <tr>
                               
                                        <th>
                                            Nombre
                                        </th>
                                        <th>
                                            Imagen
                                        </th>
                                        <th>
                                            Seleccione
                                        </th>
                                        <th>
                                           
                                        </th>
                                    </tr>
                                </thead>

                              
                                    <tbody>
                                        <tr>
                                  @foreach (Menajes_Maipu.Models.ModeloBD.Producto item in ViewBag.Sactivo)
                                  {
                                            <td>
                                                <h4>     @Html.DisplayFor(modelItem => item.Nombre_producto) </h4>
                                            </td>
                                            <td>
                                            @if (item.Imagen_producto != null)
                                            {
                                                <div style="margin-left:5px;margin-right:5px;margin-top:5px;margin-bottom:5px;">
                                                    <img width="100" height="100" src="@Url.Action("GetImage", "Home", new { item.Id_producto})" />
                                                </div>
                                            }
                                            </td>
                                  }
                                                @using (Html.BeginForm("Slider", "Dashboard", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal form-label-left" }))
                                                {
                                                    <td>
                                                    @Html.DropDownList("Principal", new SelectList(ViewBag.Producto, "Id_producto", "Nombre_producto"))
                                                    </td>
                                                    <td>
                                                        <input type="submit" value="enviar" class="btn btn-default" />
                                                    </td>
                                                }
                                                
                                           
                                        </tr>
                                    </tbody>
                          

                            </table>





